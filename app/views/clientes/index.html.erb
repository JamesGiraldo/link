<% breadcrumb :SUSCRIPTORES %>
<div class="jumbotron space-a table-responsive Admin-dark">
  <h1 class="Admin-title-dark">Adminitrador</h1>
    <div class="table-responsive">
      <table class="table table-striped table-bordered table-hover dataTables-example">
        <thead class="thead-dark">
          <tr>
            <th class="text-center  footable-visible footable-first-column footable-sortable active Admin-th-dark">Titulo</th>
            <th class="text-center  footable-visible footable-first-column footable-sortable active Admin-th-dark">Fecha</th>
            <th class="text-center  footable-visible footable-first-column footable-sortable active Admin-th-dark">Acciones</th>
          </tr>
        </thead>
        <tbody>
          <% @clientes.each do |cat| %>
            <tr class="Admin-tr-dark">
              <td class=""> <%= cat.nombre %> </td>
              <td class=""> <%= cat.created_at.strftime("%d-%m-%Y %H:%M") %> </td>
            </tr>
          <%end%>
          <% unless @clientes.present? %>
             <tr>
                <h2 class="text-center">No Hay Informacion Solicitada</h2
             </tr>
          <%end%>
        </tbody>
      </table>
    </div>
</div>
